    "dotenv": "^10.0.0",
    "fastify": "^3.22.1",
    "fastify-cookie": "^5.3.1",
    "fastify-cors": "^6.0.2",
    "fastify-formbody": "^5.1.0",
    "fastify-multipart": "^5.0.2",
    "fastify-static": "^4.4.2",
    "html-escaper": "^3.0.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.11",
    "knex-paginate": "^3.0.0",
    "memorystore": "^1.6.6",
    "ms": "^2.1.3",
